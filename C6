data = []
	data.push(["magnitude","location"])
	for(i=2;i<=13;i++){
		mag = document.querySelector(`body > div > table:nth-child(4) > tbody > tr:nth-child(${i}) > td:nth-child(5)`)
		loc = document.querySelector(`body > div > table:nth-child(4) > tbody > tr:nth-child(${i}) > td.auto-style52`)
		data.push([mag.innerText,loc.innerText])
	}
  function toCSV(data, filename){
		filename = filename + ".csv";
		let csvContent = "data:text/csv;charset=utf-8,"	
		data.forEach(function(rowArray){
		    let row = rowArray.join(",");
			csvContent += row + "\r\n";
	        console.log(row);
		});
	    
		var encodedUri = encodeURI(csvContent);
		var link = document.createElement("a");
		link.setAttribute("href",encodedUri);
		link.setAttribute("download",filename);
		document.body.appendChild(link);
		link.click()
	}
  
